<html>
    <head>
        <title>Authors feed</title>
        <link rel="stylesheet" type="text/css" href="/styles/style.css" />
    </head>
    <body id="main" class="main">
        <h2 style="position: relative; left: 40%;">Top tags of <%= name %> here!!!</h2>
        <hr style="width:50%; position: relative; left: 0%;">
        <br>
        <div id="content" class="content">
            <% if(Object.keys(arrobj).length==0){%>
                <h4>We guess <strong><%= name %></strong> hasn't said anything about particular topics, try again entering the name !!!</h4>
            <% } else { %>
                <h4>topics :
                <% Object.keys(arrobj).forEach(function(q){ %>
                    <strong><%= q %>,</strong>
                <%})%>
                </h4>
                <hr>
            <% } %>
        </div>
    </body>
</html>